spring:
  datasource:
    url: jdbc:mysql://localhost:3306/shelter_db?serverTimezone=Asia/Seoul&characterEncoding=UTF-8
    username: root
    password: 1234
    driver-class-name: com.mysql.cj.jdbc.Driver

  jpa:
    hibernate:
      ddl-auto: create   # (초기 테이블 자동 생성)
    show-sql: true
    properties:
      hibernate:
        format_sql: true

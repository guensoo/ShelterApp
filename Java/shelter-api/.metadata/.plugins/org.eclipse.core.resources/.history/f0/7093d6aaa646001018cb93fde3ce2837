import com.fasterxml.jackson.annotation.JsonProperty;
import com.shelter.shelter_api.Entity.ColdShelterEntity;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
@Builder
public class ColdShelterDTO {

    @JsonProperty("REARE_NM")
    private String reareNm;

    @JsonProperty("REARE_FCLT_NO")
    private Long reareFcltNo;

    @JsonProperty("FCLT_TYPE")
    private String fcltType;

    @JsonProperty("FCLTY_SCLAS")
    private String fcltySclas;

    @JsonProperty("DADDR")
    private String daddr;

    @JsonProperty("RONA_DADDR")
    private String ronaDaddr;

    @JsonProperty("LAT")
    private Double lat;

    @JsonProperty("LOT")
    private Double lot;

    @JsonProperty("WKDY_OPER_BGNG_HR")
    private String wkdyOperBgngHr;

    @JsonProperty("WKDY_OPER_END_HR")
    private String wkdyOperEndHr;

    @JsonProperty("LHLDY_OPER_BGNG_HR")
    private String lhhldyOperBgngHr;

    @JsonProperty("LHLDY_OPER_END_HR")
    private String lhhldyOperEndHr;

    @JsonProperty("SNDY_OPER_BGNG_HR")
    private String sndyOperBgngHr;

    @JsonProperty("SNDY_OPER_END_HR")
    private String sndyOperEndHr;

    @JsonProperty("STDY_OPER_BGNG_HR")
    private String stdyOperBgngHr;

    @JsonProperty("STDY_OPER_END_HR")
    private String stdyOperEndHr;

    @JsonProperty("UTZTN_PSBLTY_TNOP")
    private Integer utztnPsbltyTnop;

    @JsonProperty("RMRK")
    private String rmrk;

    @JsonProperty("MDFCN_HR")
    private String mdfcnHr;
    
    public ColdShelterEntity toEntity() {
        return ColdShelterEntity.builder()
            .REARE_FCLT_NO(this.REARE_FCLT_NO)
            .FCLT_TYPE(this.FCLT_TYPE)
            .REARE_NM(this.REARE_NM)
            .DADDR(this.DADDR)
            .RONA_DADDR(this.RONA_DADDR)
            .UTZTN_PSBLTY_TNOP(this.UTZTN_PSBLTY_TNOP)
            .MDFCN_HR(this.MDFCN_HR)
            .RMRK(this.RMRK)
            .WKDY_OPER_BGNG_HR(this.WKDY_OPER_BGNG_HR)
            .WKDY_OPER_END_HR(this.WKDY_OPER_END_HR)
            .SNDY_OPER_BGNG_HR(this.SNDY_OPER_BGNG_HR)
            .SNDY_OPER_END_HR(this.SNDY_OPER_END_HR)
            .STDY_OPER_BGNG_HR(this.STDY_OPER_BGNG_HR)
            .STDY_OPER_END_HR(this.STDY_OPER_END_HR)
            .LHLDY_OPER_BGNG_HR(this.LHLDY_OPER_BGNG_HR)
            .LHLDY_OPER_END_HR(this.LHLDY_OPER_END_HR)
            .FCLTY_SCLAS(this.FCLTY_SCLAS)
            .LOT(this.LOT)
            .LAT(this.LAT)
            .build();
    }
}
